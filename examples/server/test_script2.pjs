            <scriptInit># Objects for access corresponding device memory: mem0x, mem1x, mem3x, mem4x.
#
# Every object has set of get/set function to work with different data types:
#  * mem0x, mem1x: `get&lt;datatype&gt;(bitoffset:int)-&gt;(int, float)` and `set&lt;datatype&gt;(bitoffset:int,value:(int, float))`
#  * mem3x, mem4x: `get&lt;datatype&gt;(regoffset:int)-&gt;(int, float)` and `set&lt;datatype&gt;(regoffset:int,value:(int, float))`
#
# &lt;datatype&gt;: int8, uint8, int16, uint16, int32, uint32, int64, uint64, float, double.
#
# Examples:
#  v = mem0x.getint8(0)
#  mem1x.setint16(1, -1)
#  mem3x.setuint16(0, 65535)
#  mem3x.setfloat(0, 3.14)
#  mem4x.setdouble(10, 2.71828)

print(f&quot;{mem0x.getint16(0)=}&quot;)
print(f&quot;{mem0x.getuint16(0)=}&quot;)
print(f&quot;{mem0x.getint32(0)=}&quot;)
print(f&quot;{mem0x.getuint32(0)=}&quot;)
print(f&quot;{mem0x.getint64(0)=}&quot;)
print(f&quot;{mem0x.getuint64(0)=}&quot;)
print(f&quot;{mem0x.getfloat(0)=}&quot;)
print(f&quot;{mem0x.getdouble(0)=}&quot;)
print(f&quot;{mem0x.getstring(900, 12)=}&quot;)
print(&quot;--------------------------------&quot;)
print(f&quot;{mem4x.getint16(0)=}&quot;)
print(f&quot;{mem4x.getuint16(0)=}&quot;)
print(f&quot;{mem4x.getint32(0)=}&quot;)
print(f&quot;{mem4x.getuint32(0)=}&quot;)
print(f&quot;{mem4x.getint64(0)=}&quot;)
print(f&quot;{mem4x.getuint64(0)=}&quot;)
print(f&quot;{mem4x.getfloat(0)=}&quot;)
print(f&quot;{mem4x.getdouble(0)=}&quot;)
print(f&quot;{mem4x.getstring(90, 12)=}&quot;)
print(&quot;################################&quot;)

mem0x.setint16 (100, -5)
mem0x.setuint16(200, 32769)
mem0x.setint32 (300, -5)
mem0x.setuint32(400, 3000000000)
mem0x.setint64 (500, -5)
mem0x.setuint64(600, 0x8877665544332211)
mem0x.setfloat (700, 3.14)
mem0x.setdouble(800, 2.718281828)
mem0x.setstring(900, &quot;Hello world&quot;)

mem4x.setint16 (10, -5)
mem4x.setuint16(20, 32769)
mem4x.setint32 (30, -5)
mem4x.setuint32(40, 3000000000)
mem4x.setint64 (50, -5)
mem4x.setuint64(60, 0x8877665544332211)
mem4x.setfloat (70, 3.14)
mem4x.setdouble(80, 2.718281828)
mem4x.setstring(90, &quot;Hello world&quot;)</scriptInit>
